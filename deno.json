{
  "tasks": {
    "chel": "deno task build && deno run --allow-net --allow-read=. --allow-write=. build/main.js",
    "lint": "standard ./src",
    "compile": "deno run --allow-run --allow-read=. --allow-write=./dist scripts/compile.ts",
    "build": "deno run --node-modules-dir --allow-run --allow-read --allow-env --allow-write=./build --allow-net scripts/build.ts",
    "dist": "deno lint . && deno task build && deno task compile",
    "test": "deno test --allow-read=. --allow-write=test"
  },
  "imports": {
    "~/": "./src/",
    "jsr:@std/": "https://jsr.io/@std/",
    "https://esm.sh/tweetnacl@1.0.3?pin=v120": "https://esm.sh/tweetnacl@1.0.3?pin=v120",
    "https://esm.sh/multiformats@11.0.2/bases/base58?pin=v120": "https://esm.sh/multiformats@11.0.2/bases/base58?pin=v120",
    "https://esm.sh/multiformats@11.0.2?pin=v120": "https://esm.sh/multiformats@11.0.2?pin=v120",
    "https://esm.sh/@multiformats/blake2@1.0.13?pin=v120": "https://esm.sh/@multiformats/blake2@1.0.13?pin=v120",
    "https://esm.sh/multiformats@11.0.2/cid?pin=v120": "https://esm.sh/multiformats@11.0.2/cid?pin=v120",
    "https://deno.land/x/miniild@v0.14.47/mod.js": "https://deno.land/x/esbuild@v0.14.47/mod.js",
    "https://deno.land/x/sqlite@v3.7.1/mod.ts": "https://deno.land/x/sqlite@v3.7.1/mod.ts"
  },
  "compilerOptions": {
  },
  "fmt": {
    "lineWidth": 100,
    "semiColons": false,
    "singleQuote": true
  },
  "lint": {
    "exclude": ["node_modules/", "vendor/", "dist/", "build/", "test/assets/**/*.js"]
  },
  "vendor": true
}
